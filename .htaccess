<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /sweetydog/

  # Canonicaliser les URLs pour ne pas exposer /public
  RewriteRule ^public/(.*)$ /Sweetydog/$1 [R=301,L]

  # Si le fichier ou dossier existe Ã  la racine, on le sert
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]

  # Sinon -> on envoie vers public/
  RewriteRule ^(.*)$ public/$1 [L]
</IfModule>
